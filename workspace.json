{
  "version": 1,
  "projects": {
    "mf": {
      "root": "packages/mf",
      "sourceRoot": "packages/mf/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/mf/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/mf/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/packages/mf"]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/mf",
            "tsConfig": "packages/mf/tsconfig.lib.json",
            "packageJson": "packages/mf/package.json",
            "main": "packages/mf/src/index.ts",
            "assets": [
              {
                "input": "./packages/mf/tutorial",
                "glob": "**/*",
                "output": "tutorial"
              },
              "packages/mf/*.md",
              {
                "input": "./packages/mf/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/mf",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/mf",
                "glob": "builders.json",
                "output": "."
              }
            ]
          },
          "outputs": ["{options.outputPath}"]
        }
      }
    },
    "mf-e2e": {
      "projectType": "application",
      "root": "e2e\\mf-e2e",
      "sourceRoot": "e2e\\mf-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "mf:build",
            "npmPackageName": "@angular-architects/mf",
            "pluginOutputPath": "dist/packages/mf",
            "jestConfig": "e2e/mf-e2e/jest.config.js"
          }
        }
      }
    },
    "mf-utils": {
      "root": "packages/mf-utils",
      "sourceRoot": "packages/mf-utils/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/mf-utils/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/mf-utils"],
          "options": {
            "jestConfig": "packages/mf-utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace",
    "packageManager": "yarn"
  }
}
